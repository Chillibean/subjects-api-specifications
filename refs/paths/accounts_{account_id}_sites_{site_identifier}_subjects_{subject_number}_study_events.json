{
  "post": {
    "summary": "Create an unscheduled study event",
    "description": "Create an unscheduled study event",
    "operationId": "createStudyEvent",
    "tags": [
      "Study Events"
    ],
    "security": [
      {
        "userSessionKey": [],
        "chillipharmSession": []
      }
    ],
    "parameters": [
      {
        "name": "account_id",
        "in": "path",
        "description": "The account id",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      },
      {
        "name": "site_identifier",
        "in": "path",
        "description": "The site identifier",
        "required": true,
        "schema": {
          "type": "string",
          "example": "SITE1"
        }
      },
      {
        "name": "subject_number",
        "in": "path",
        "description": "The subject number",
        "required": true,
        "schema": {
          "type": "string",
          "example": "1"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "authenticity_token": {
                "type": "string",
                "description": "The authenticity token",
                "example": "1234567890"
              },
              "date": {
                "type": "string",
                "description": "The date this unscheduled study event took place",
                "example": "2024-01-03"
              },
              "study_procedure_definition_identifiers": {
                "type": "array",
                "description": "The procedures that took place during this unscheduled study event",
                "example": [
                  "SIXWALK",
                  "PUL",
                  "NSTAR"
                ]
              }
            },
            "required": [
              "authenticity_token"
            ],
            "additionalProperties": false
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "OK",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "study_event": {
                  "$ref": "../components/schemas/StudyEvent.json"
                },
                "schedule": {
                  "type": "array",
                  "items": {
                    "$ref": "../components/schemas/StudyEvent.json"
                  }
                }
              },
              "required": [
                "schedule",
                "study_event"
              ],
              "additionalProperties": false
            }
          }
        }
      },
      "400": {
        "$ref": "../components/responses/BadRequest.json"
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      },
      "403": {
        "$ref": "../components/responses/Forbidden.json"
      },
      "404": {
        "$ref": "../components/responses/NotFound.json"
      },
      "409": {
        "$ref": "../components/responses/Conflict.json"
      },
      "500": {
        "$ref": "../components/responses/InternalServerError.json"
      }
    }
  }
}